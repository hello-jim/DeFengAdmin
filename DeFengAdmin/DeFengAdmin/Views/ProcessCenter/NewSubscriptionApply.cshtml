
@{
    ViewBag.Title = "NewSubscriptionApply";
}
<link rel="stylesheet" href="~/Scripts/datetimepicker/bootstrap-datetimepicker.min.css"> 
<div class="panel">
<div class="row text-right"><a>送办</a>丨<a>查看流程</a>丨<a>暂存</a>丨<a href="/ProcessCenter/NewSubscription">返回</a></div>
<div class="row">&nbsp;</div>
<div class="row text-center"><h3>一手房认购单</h3></div>
<div class="row">&nbsp;</div>
<table class="table table-bordered staffTable table-small-font">
    <tr>
        <td>项目</td>
        <td>栋</td>
        <td colspan="2">单元</td>
        <td>房号</td>
    </tr>
    <tr>
        <td>
            <select class="form-control" id="application_method">
                <option></option>
            </select>
        </td>
        <td><input type="text" class="form-control input-dark" name="title" id="title" /></td>
        <td colspan="2"><input type="text" class="form-control input-dark" name="title" id="title" /></td>
        <td><input type="text" class="form-control input-dark" name="title" id="title" /></td>
    </tr>
    <tr>
        <td>客户姓名</td>
        <td>客户联系号码</td>
        <td>定金</td>
        <td>认购时间</td>
        <td>认购总价</td>
    </tr>
    <tr>
        <td><input type="text" class="form-control input-dark" name="title" id="title" /></td>
        <td><input type="text" class="form-control input-dark" name="title" id="title" /></td>
        <td><input type="text" class="form-control input-dark" name="title" id="title" /></td>
        <td>
            <div class="date form_datetime">
                <input id="submitHouseDate" class="form-control" name="submitHouseDate" data-validate="required" readonly />
                <span class="add-on"><i class="icon-remove"></i></span>
                <span class="add-on"><i class="icon-calendar"></i></span>
            </div>
        </td>
        <td><input type="text" class="form-control input-dark" name="title" id="title" /></td>
    </tr>
    <tr>
        <td>业绩模式</td>
        <td colspan="10">
            <label class="radio-inline">
                <input type="radio" value="门店" name="market">
                门店
            </label>


            <label class="radio-inline">
                <input type="radio" value="外勤" name="online">
                外勤
            </label>


            <label class="radio-inline">
                <input type="radio" value="外勤+驻点" name="posted">
                外勤+驻点
            </label>


            <label class="radio-inline">
                <input type="radio" value="门店+驻点" name="serve">
                门店+驻点
            </label>


            <label class="radio-inline">
                <input type="radio" value="联动模式" name="recommend">
                联动模式
            </label>
        </td>
    </tr>
    <tr>
        <td rowspan="6">销售人员</td>
        <td rowspan="3">送客方</td>
        <td>经纪人</td>
        <td colspan="3"><input type="text" class="form-control input-dark" name="title" id="title" /></td>
    </tr>
    <tr>
        <td>分行经理</td>
        <td colspan="3"><input type="text" class="form-control input-dark" name="title" id="title" /></td>
    </tr>
    <tr>
        <td>现场经理</td>
        <td colspan="3"><input type="text" class="form-control input-dark" name="title" id="title" /></td>
    </tr>
    <tr>
        <td rowspan="3">驻点</td>
        <td>经纪人</td>
        <td colspan="3"><input type="text" class="form-control input-dark" name="title" id="title" /></td>
    </tr>
    <tr>
        <td>分行经理</td>
        <td colspan="3"><input type="text" class="form-control input-dark" name="title" id="title" /></td>
    </tr>
    <tr>
        <td>现场经理</td>
        <td colspan="3"><input type="text" class="form-control input-dark" name="title" id="title" /></td>
    </tr>
    <tr>
        <td>备注</td>
        <td colspan="8"><input type="text" class="form-control input-dark" name="title" id="title" /></td>
    </tr>
    <tr>
        <td>附件</td>
        <td colspan="8"><input type="text" class="form-control input-dark" name="title" id="title" /></td>
    </tr>
    <tr>
        <td>店长或外勤经理</td>
        <td colspan="8"><input type="text" class="form-control input-dark" name="title" id="title" /></td>
    </tr>
    <tr>
        <td>现场经理</td>
        <td colspan="8"><input type="text" class="form-control input-dark" name="title" id="title" /></td>
    </tr>
    <tr>
        <td>刘洋</td>
        <td colspan="8"><input type="text" class="form-control input-dark" name="title" id="title" /></td>
    </tr>
</table>
</div>
<script src="/Scripts/daterangepicker/daterangepicker.js"></script>
<script src="/Scripts/datepicker/bootstrap-datepicker.js"></script>
<script src="/Scripts/timepicker/bootstrap-timepicker.min.js"></script>
<script src="~/Scripts/datetimepicker/bootstrap-datetimepicker.min.js"></script>
<script type="text/javascript" src="/Scripts/datetimepicker/locales/bootstrap-datetimepicker.zh-CN.js"></script>
<script type="text/javascript">
        $(document).ready(function () {
            $('.form_datetime').datetimepicker({
                format: 'yyyy-mm-dd',
                language: 'zh-CN',
                // weekStart: 1,
                todayBtn: 1,
                autoclose: 1,
                todayHighlight: 1,
                startView: 2,
                minView: 2,
                forceParse: 0
            });
        });

</script>
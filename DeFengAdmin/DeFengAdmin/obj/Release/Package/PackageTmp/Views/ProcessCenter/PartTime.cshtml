
@{
    ViewBag.Title = "PartTime";
}
<link rel="stylesheet" href="~/Scripts/datetimepicker/bootstrap-datetimepicker.min.css"> 
<div class="panel">
    <div class="row text-right"><a>送办</a>丨<a>查看流程</a>丨<a>暂存</a>丨<a>打印</a>丨<a href="/ProcessCenter/PartTimeJob">返回</a></div>
    <div class="row text-center">
        <h3>兼职申请</h3>
    </div>
    <div class="row">&nbsp;</div>
    <table class="table table-bordered staffTable table-small-font">
        <tr>
            <td>申请人员</td>
            <td><input type="text" class="form-control input-dark" name="number" id="number" autocomplete="off" /></td>
            <td>所属部门</td>
            <td><input type="text" class="form-control input-dark" name="number" id="number" autocomplete="off" /></td>
            <td>申请时间</td>
            <td>
                <div class="date form_datetime">
                    <input id="submitHouseDate" class="form-control" name="submitHouseDate" data-validate="required" readonly />
                    <span class="add-on"><i class="icon-remove"></i></span>
                    <span class="add-on"><i class="icon-calendar"></i></span>
                </div>
            </td>
        </tr>
        <tr>
            <td>附件</td>
            <td colspan="8"><input type="text" class="form-control input-dark" name="title" id="title" /></td>
        </tr>
        <tr>
            <td colspan="10" class="text-center">同公司兼职</td>
        </tr>
        <tr>
            <td colspan="10"> <a href="#">新建</a>丨<a href="#">删除</a></td>
        </tr>
        <tr>
            <td colspan="10">
            <table cellspacing="0" class="table table-small-font table-bordered table-striped">
                <thead>
                    <tr>
                        <th>
                            <div class="cbr-replaced">
                                <div class="cbr-input">
                                    <input type="checkbox" class="cbr cbr-done">
                                </div>
                                <div class="cbr-state"><span></span></div>
                            </div>
                        </th>
                        <th>用户</th>
                        <th>原单位</th>
                        <th>原部门</th>
                        <th>原岗位</th>
                        <th>兼职部门</th>
                        <th>兼职岗位</th>
                    </tr>
                </thead>
                <tbody class="collaborative">
                    <tr></tr>
                </tbody>
            </table>
                </td>
        </tr>
        <tr>
            <td colspan="10" class="text-center">不同公司兼职</td>
        </tr>
        <tr>
            <td colspan="10"> <a href="#">新建</a>丨<a href="#">删除</a></td>
        </tr>
        <tr>
            <td colspan="10">
                <table cellspacing="0" class="table table-small-font table-bordered table-striped">
                    <thead>
                        <tr>
                            <th>
                                <div class="cbr-replaced">
                                    <div class="cbr-input">
                                        <input type="checkbox" class="cbr cbr-done">
                                    </div>
                                    <div class="cbr-state"><span></span></div>
                                </div>
                            </th>
                            <th>用户</th>
                            <th>原单位</th>
                            <th>原部门</th>
                            <th>原岗位</th>
                            <th>兼职部门</th>
                            <th>兼职主部门</th>
                        </tr>
                    </thead>
                    <tbody class="collaborative">
                        <tr></tr>
                    </tbody>
                </table>
            </td>
        </tr>
        <tr>
            <td>备注 </td>
            <td colspan="8"><input type="text" class="form-control input-dark" name="title" id="title" /></td>
        </tr>
        <tr>
            <td>上级领导意见</td>
            <td colspan="8"><input type="text" class="form-control input-dark" name="title" id="title" /></td>
        </tr>
        <tr>
            <td>人事经理意见 </td>
            <td colspan="8"><input type="text" class="form-control input-dark" name="title" id="title" /></td>
        </tr>
        <tr>
            <td>总经理意见 </td>
            <td colspan="8"><input type="text" class="form-control input-dark" name="title" id="title" /></td>
        </tr>
    </table>
</div>

<script src="/Scripts/daterangepicker/daterangepicker.js"></script>
<script src="/Scripts/datepicker/bootstrap-datepicker.js"></script>
<script src="/Scripts/timepicker/bootstrap-timepicker.min.js"></script>
<script src="~/Scripts/datetimepicker/bootstrap-datetimepicker.min.js"></script>
<script type="text/javascript" src="/Scripts/datetimepicker/locales/bootstrap-datetimepicker.zh-CN.js"></script>
<script type="text/javascript">
        $(document).ready(function () {
            $('.form_datetime').datetimepicker({
                format: 'yyyy-mm-dd',
                language: 'zh-CN',
                // weekStart: 1,
                todayBtn: 1,
                autoclose: 1,
                todayHighlight: 1,
                startView: 2,
                minView: 2,
                forceParse: 0
            });
        });

</script>